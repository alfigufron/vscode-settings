	"dart.flutterSdkPath": "C:\\flutter",
	"workbench.startupEditor": "newUntitledFile",
	"explorer.confirmDelete": false,
	"liveServer.settings.donotVerifyTags": true,
	"editor.minimap.enabled": false,
	"liveServer.settings.donotShowInfoMsg": true,
	"[json]": {
		"editor.quickSuggestions": {
			"strings": true
		},
		"editor.suggest.insertMode": "replace"
	},
	"json.schemas": [],
	"dart.checkForSdkUpdates": false,
	// "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
	"eslint.validate": [
		"html",
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact"
	],
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": "explicit",
		"source.fixAll": "explicit"
	},
	"breadcrumbs.enabled": true,
	"update.enableWindowsBackgroundUpdates": false,
	"update.mode": "manual",
	// "[html]": {
	//   "editor.defaultFormatter": "HookyQR.beautify"
	// },
	"editor.suggestSelection": "first",
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"javascript.updateImportsOnFileMove.enabled": "always",
	"workbench.editor.enablePreview": true,
	// "javascript.validate.enable": false,
	// "files.associations": {
	//   "*.js": "javascriptreact",
	//   "*.tsx": "javascript"
	// },
	// "[javascriptreact]": {
	//   "editor.defaultFormatter": "esbenp.prettier-vscode"
	// },
	// "[typescript]": {
	//   "editor.defaultFormatter": "esbenp.prettier-vscode"
	// },
	"editor.formatOnSave": true,
	"liveSassCompile.settings.formats": [
		{
			"format": "expanded",
			"extensionName": ".css",
			"savePath": "~./css/"
		}
	],
	"files.exclude": {
		"**/.classpath": true,
		"**/.project": true,
		"**/.settings": true,
		"**/.factorypath": true
	},
	"launch": {
		"configurations": [],
		"compounds": []
	},
	"terminal.integrated.defaultProfile.windows": "Git Bash",
	// "security.workspace.trust.untruste/dFiles": "newWindow",
	"files.maxMemoryForLargeFilesMB": 8096,
	"editor.unicodeHighlight.nonBasicASCII": false,
	"npm.keybindingsChangedWarningShown": true,
	"prettier.endOfLine": "crlf",
	"tailwindCSS.includeLanguages": {
		"plaintext": "html"
	},
	"terminal.integrated.env.osx": {
		"FIG_NEW_SESSION": "1"
	},
	"editor.accessibilitySupport": "off",
	"editor.bracketPairColorization.enabled": false,
	"prettier.useTabs": true,
	"editor.tabSize": 2,
	"scss.lint.unknownAtRules": "ignore",
	"css.lint.unknownAtRules": "ignore",

	"discord.removeRemoteRepository": true,
	// "discord.detailsEditing": "üìù Writing: {file_name}",
	"discord.detailsEditing": "üìù Writing: Confidential üîí",
	"discord.detailsIdling": "üßò Take short break",
	"discord.lowerDetailsEditing": "üóÇÔ∏è {full_dir_name}",
	// "discord.lowerDetailsEditing": "üóÇÔ∏è Confidential üîí",
	"discord.suppressNotifications": true,
	"discord.removeTimestamp": false,
	"diffEditor.hideUnchangedRegions.enabled": true,
	"workbench.editor.empty.hint": "hidden",
	"workbench.colorTheme": "Mayukai Alucard",
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"workbench.iconTheme": "material-icon-theme",
	"window.zoomLevel": -0.8,
	// "window.zoomLevel": 1,
	"editor.stickyScroll.enabled": true,
	"editor.inlineSuggest.showToolbar": "onHover",
	"typescript.updateImportsOnFileMove.enabled": "always",
	"dart.openDevTools": "flutter",
	"workbench.tree.indent": 14,
	"workbench.tree.renderIndentGuides": "always",
	"[html]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	}
}
